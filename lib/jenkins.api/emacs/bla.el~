(defun jenkins-read-job (prompt)
  ""
  (completing-read prompt (slime-eval '(jenkins.management::all-jobs))))

(defun jenkins-copy-job (source-name new-name)
  ""
  (interactive (let ((source-name (jenkins-read-job "Copy job: ")))
		 (list source-name
		       (read-string "Name of new job: " source-name))))
  (list source-name new-name))

(provide 'bla)
;;; bla.el ends here
